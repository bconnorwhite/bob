{
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env", {
        "loose": true,
        "exclude": [
          "@babel/plugin-transform-regenerator"
        ]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-replace-ts-export-assignment",
    "@babel/plugin-proposal-class-properties",
  ]
}
